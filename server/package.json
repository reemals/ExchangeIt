{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www | pino-pretty",
    "dev:debug": "nodemon --inspect ./bin/www",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.9.27",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.4.11"
  },
  "devDependencies": {
    "jest": "^26.2.2",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.4",
    "pino": "^6.5.0",
    "pino-pretty": "^4.1.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./tests/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "client"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": [
      "html"
    ],
    "coverageDirectory": "./coverage/"
  }
}
